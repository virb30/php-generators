{
    "name": "project/project",
    "type": "project",
    "decription": "Test with php generators",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Db\\": "db/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpro/grumphp": "^1.15",
        "friendsofphp/php-cs-fixer": "^3.15",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "require": {
        "robmorgan/phinx": "^0.13.4",
        "fzaninotto/faker": "^1.5"
    }
}